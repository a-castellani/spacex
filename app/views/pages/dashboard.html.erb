<div class="row justify-content-center">
    <div class="col-6 text-center">
      <h3>My Bookings</h3>
      <% @current_user_bookings.each do |booking| %>
      <div><%= booking.spaceship.name %></div>
      <% end %>
    </div>
    <div class="col-6 text-center">
      <h3>My Spaceships</h3>
      <% @current_user_spaceships.each do |spaceship| %>
