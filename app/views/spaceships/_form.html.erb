<%= form_with model: @spaceship do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :color %>
  <%= f.text_field :color %>

  <%= f.label :seats %>
  <%= f.text_field :seats %>

  <%= f.label :year %>
  <%= f.text_field :year %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit %>
<% end %>
